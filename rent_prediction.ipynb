import torch
import pandas as pd
import numpy as np
import torch.nn as nn
import torch.optim as optim
from sklearn.model_selection import train_test_split

street_easy_df = pd.read_csv("streeteasy.csv")
street_easy_df.head()

numerical_features = ['bedrooms', 'bathrooms', 'size_sqft', 'min_to_subway', 'floor', 'building_age_yrs',
                      'no_fee', 'has_roofdeck', 'has_washer_dryer', 'has_doorman', 'has_elevator', 'has_dishwasher',
                      'has_patio', 'has_gym']

X = torch.tensor(street_easy_df[numerical_features].values, dtype=torch.float)
y = torch.tensor(street_easy_df['rent'].values, dtype=torch.float).view(-1,1)

X_train, X_test, y_train, y_test = train_test_split(X, y,
    train_size=0.80,
    test_size=0.20,
    random_state=2) 

# set a random seed - do not modify
torch.manual_seed(42)

## YOUR SOLUTION HERE ##
model = nn.Sequential(
            nn.Linear(14,28),
            nn.ReLU(),
            nn.Linear(28,10),
            nn.GELU(),
            nn.Linear(10,1),
)

# show model details
model

loss = nn.MSELoss()

import torch.optim as optim

loss_fn = nn.L1Loss()
optimizer = torch.optim.Adam(model.parameters(), lr=0.001)

num_epochs = 500
for epoch in range(num_epochs):
    predictions = model(X_train)
    loss = loss_fn(predictions, y_train)

    optimizer.zero_grad()
    loss.backward()
    optimizer.step()

    if (epoch + 1) % 100 == 0:
        print(f'Epoch [{epoch + 1}/{num_epochs}], L1 Loss: {loss.item():.2f}')


model.eval()
with torch.no_grad():
    test_preds = model(X_test)
    test_loss = loss_fn(test_preds, y_test)
    print("Test loss:", test_loss.item())
